{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "morpho",
      "camelcase": "Morpho",
      "title": "Morpho",
      "scope": "source.morpho",
      "file-types": [
        "morpho"
      ],
      "injection-regex": "^morpho$",
      "class-name": "TreeSitterMorpho",
      "highlights": "queries/highlights.scm"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "A simple tree-sitter parser for Morpho",
    "authors": [
      {
        "name": "Emmanuel Flores",
        "email": "eq.emmanuel.137@gmail.com",
        "url": ""
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-morpho",
      "funding": ""
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "java": false,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}